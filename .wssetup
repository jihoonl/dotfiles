function indigobase {
  echo "Setting Indigo base..."
  source /opt/ros/indigo/setup.bash
  rospack profile
}

function ros_beginners {
  echo "Setting ROS Beginers.."
  source ~/research/ros/ros_beginners/devel/setup.bash
  rospack profile
}

function grpws {
  echo "Setting grp.."
  source ~/research/ros/grp/devel/setup.bash
  rospack profile
}
